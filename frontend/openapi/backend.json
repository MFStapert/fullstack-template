{
  "openapi": "3.0.0",
  "paths": {
    "/healthcheck": {
      "get": {
        "operationId": "AppController_healthcheck",
        "parameters": [],
        "responses": {
          "200": {
            "description": "",
            "content": { "application/json": { "schema": { "type": "string" } } }
          }
        }
      }
    },
    "/meets": {
      "post": {
        "operationId": "MeetController_createMeet",
        "parameters": [],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": { "schema": { "$ref": "#/components/schemas/CreateMeetDto" } }
          }
        },
        "responses": {
          "201": {
            "description": "",
            "content": {
              "application/json": { "schema": { "$ref": "#/components/schemas/MeetDetailDto" } }
            }
          }
        },
        "tags": ["Meets"]
      }
    },
    "/meets/{meetId}": {
      "get": {
        "operationId": "MeetController_getMeet",
        "parameters": [
          { "name": "meetId", "required": true, "in": "path", "schema": { "type": "number" } }
        ],
        "responses": {
          "200": {
            "description": "",
            "content": {
              "application/json": { "schema": { "$ref": "#/components/schemas/MeetDetailDto" } }
            }
          }
        },
        "tags": ["Meets"]
      }
    },
    "/meets/by-user/{userId}": {
      "get": {
        "operationId": "MeetController_getMeetsByUser",
        "parameters": [
          { "name": "userId", "required": true, "in": "path", "schema": { "type": "number" } }
        ],
        "responses": {
          "200": {
            "description": "",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": { "$ref": "#/components/schemas/MeetOverviewDto" }
                }
              }
            }
          }
        },
        "tags": ["Meets"]
      }
    },
    "/meets/{meetId}/vote": {
      "post": {
        "operationId": "MeetController_createVote",
        "parameters": [
          { "name": "meetId", "required": true, "in": "path", "schema": { "type": "number" } }
        ],
        "requestBody": {
          "required": true,
          "content": {
            "application/json": { "schema": { "$ref": "#/components/schemas/CreateVoteDto" } }
          }
        },
        "responses": { "201": { "description": "" } },
        "tags": ["Meets"]
      }
    },
    "/locations": {
      "get": {
        "operationId": "LocationController_getLocations",
        "parameters": [],
        "responses": {
          "200": {
            "description": "",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": { "$ref": "#/components/schemas/LocationDto" }
                }
              }
            }
          }
        },
        "tags": ["Locations"]
      }
    },
    "/users": {
      "get": {
        "operationId": "UserController_getUsers",
        "parameters": [],
        "responses": {
          "200": {
            "description": "",
            "content": {
              "application/json": {
                "schema": { "type": "array", "items": { "$ref": "#/components/schemas/UserDto" } }
              }
            }
          }
        },
        "tags": ["Users"]
      }
    },
    "/users/{id}": {
      "get": {
        "operationId": "UserController_getUser",
        "parameters": [
          { "name": "id", "required": true, "in": "path", "schema": { "type": "number" } }
        ],
        "responses": {
          "200": {
            "description": "",
            "content": {
              "application/json": { "schema": { "$ref": "#/components/schemas/UserDto" } }
            }
          }
        },
        "tags": ["Users"]
      }
    }
  },
  "info": { "title": "Backend API", "description": "", "version": "1.0", "contact": {} },
  "tags": [],
  "servers": [{ "url": "http://localhost/api" }],
  "components": {
    "schemas": {
      "CreateMeetDto": {
        "type": "object",
        "properties": {
          "title": { "type": "string" },
          "createdBy": { "type": "number" },
          "users": { "type": "array", "items": { "type": "number" } }
        },
        "required": ["title", "createdBy", "users"]
      },
      "MeetDetailDto": {
        "type": "object",
        "properties": {
          "id": { "type": "number" },
          "title": { "type": "string" },
          "locationTitle": { "type": "string" },
          "createdBy": { "type": "number" },
          "userNames": { "type": "array", "items": { "type": "string" } }
        },
        "required": ["id", "title", "locationTitle", "createdBy", "userNames"]
      },
      "MeetOverviewDto": {
        "type": "object",
        "properties": { "id": { "type": "number" }, "title": { "type": "string" } },
        "required": ["id", "title"]
      },
      "CreateVoteDto": {
        "type": "object",
        "properties": { "createdBy": { "type": "number" }, "locationId": { "type": "number" } },
        "required": ["createdBy", "locationId"]
      },
      "LocationDto": {
        "type": "object",
        "properties": { "id": { "type": "number" }, "title": { "type": "string" } },
        "required": ["id", "title"]
      },
      "UserDto": {
        "type": "object",
        "properties": { "id": { "type": "number" }, "name": { "type": "string" } },
        "required": ["id", "name"]
      }
    }
  }
}
